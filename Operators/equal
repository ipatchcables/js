const numberFive = 5;
console.log(typeof numberFive)

const stringFive = "5"
console.log(typeof stringFive)

console.log(numberFive === stringFive)

console.log(numberFive == stringFive) // will change type


if (numberFive == stringFive) {
  console.log("hi")
} else {
  console.log("falsE")
}


