//Variables
//Cannot asign vars to reserved keywords (duh)
var myName = "Chris";
var myLastName = "Derp";

console.log(myName + " " + myLastName)

var number = 2;
console.log(number)

var myArray = [1,2,3,4,5];
console.log(myArray.length - 1);

var myObject = {
  name: "Chris",
  age: 32,
};

console.log(typeof myObject);
console.log(myObject);
console.log(myObject);

function printOut() {
  console.log(myObject)
}
printOut()

var count = 1;
function increase(){
  count += 1;
  console.log(count)
}

var myName = "bpb";
var amChris = false;
console.log(amChris); 

function sayName(){ // scope change
  var myName = "Derp"
  console.log(myName);
}
sayName()
