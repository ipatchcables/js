var myNamesArray = new Array();
myNamesArray[0] = "Chris";
myNamesArray[1] = "Chris2";
console.log(myNamesArray)

var myNamesArray = new Array();
myNamesArray[0] = "Chris";
myNamesArray[1] = "Chris2";
console.log(myNamesArray.length)

var myNamesArray = ["derp", "jessica", john"]; // push into array
myNamesArray.push("poop");
console.log(myNamesArray);

var myNamesArray = ["derp", "jessica", "john"]; // pop off array
myNamesArray.pop;
console.log(myNamesArray);

var myNamesArray = ["derp", "jessica", "john", ["blue", "orange", "dick"]];
console.log(myNamesArray[3][1]); // index array
var myNamesArray = ["derp", "jessica", "john"];
var sortedArray = myNamesArray.sort();
console.log(sortedArray);

var myNamesArray = ["derp", "jessica", "john"];
var myMoreNamesArray = ["te", "sa", "a"];
var concat = myNamesArray.concat(myMoreNamesArray);
console.log(concat);

var slicedArray = concat.slice(5,6,) //slice array

var myNamesArray = ["derp", "jessica", "john"];
var joinedArray = myNamesArray.join(" [pp[[- ]]]"); // add into array
console.log(joinedArray);


var myNamesArray = ["derp", "jessica", "john"];
var slicedArray = myNamesArray.slice(myNamesArray.length -1); // find last
console.log(slicedArray);
